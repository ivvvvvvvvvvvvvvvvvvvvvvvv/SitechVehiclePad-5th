<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fth"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/transparent">

    <com.sitechdev.vehicle.pad.view.ZSideBar
        android:id="@+id/zbar"
        android:layout_width="1300px"
        android:layout_height="70px"
        android:background="@color/bbs_color_pri"
        android:layout_gravity="center_horizontal"
        tools:layout_width="1100px"
        tools:layout_height="70px" />

    <com.sitechdev.vehicle.pad.view.RecycviewInViewPager
        android:id="@+id/recyclerView"
        android:background="@color/color_main_weather_status_desc_range"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>