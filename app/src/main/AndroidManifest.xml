<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.sitechdev.vehicle.pad"
    android:sharedUserId="android.uid.system">

    <application
        android:name=".app.AppApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:allowBackup">
        <activity
            android:name=".module.feedback.FeedbackHistoryActivity"
            android:screenOrientation="landscape" />

        <activity android:name=".module.weather.WeatherActivity" />
        <activity
            android:name=".module.taxi.TaximeterActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".module.setting.activity.SettingSkinActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".module.member.MemberPreActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".module.carstatus.CarStatusPreActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".module.apps.view.AppsListGridActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".module.splash.SplashActivity"
            android:clearTaskOnLaunch="true"
            android:enabled="true"
            android:launchMode="singleTask"
            android:resizeableActivity="true"
            android:resumeWhilePausing="true"
            android:stateNotNeeded="true"
            android:theme="@style/NoTitleAppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.HOME" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 高德key -->
        <activity
            android:name=".module.main.MainActivity"
            android:exported="true"
            android:launchMode="singleTask"
            android:theme="@style/NoTitleAppTheme">
            <intent-filter>
                <!-- 兼容高德地图无网络跳转-->
                <action android:name="com.sitechdev.vehicle" />
                <!--                <action android:name="android.intent.action.MAIN" />-->
                <!--                <category android:name="android.intent.category.HOME" />-->
                <!--                <category android:name="android.intent.category.DEFAULT" />-->
                <!--                <category android:name="android.intent.category.LAUNCHER" />-->
            </intent-filter>
        </activity>

        <activity
            android:name=".module.map.MapActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.map.PoiSearchActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.map.SetAddressActivity"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.member.MemberActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.member.AboutActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.member.FeedbackActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.member.MyPointActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.forshow.MusicMainForShowActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.forshow.AudioListForShowActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.setting.activity.SettingNetActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.setting.activity.SettingHomeActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.setting.activity.SettingSystemActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.setting.activity.SettingTeddyActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.setting.activity.SettingBtActivity"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleAppTheme" />
        <activity
            android:name=".module.online_audio.KaolaAudioActivity"
            android:launchMode="singleTask" />
        <activity android:name=".module.online_audio.MusicKaolaActivity" />
        <activity android:name=".module.online_audio.MusicKaolaBroadcastActivity" />
        <activity android:name=".module.feedback.FeedbackActivity" />
        <activity android:name=".module.main.DebugInfo" />
        <activity
            android:name=".module.music.MusicMainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".module.video.VideoListActivity" />
        <activity android:name=".module.phone.PhoneActivity" />
        <activity android:name=".module.led.activity.LedActivity" />

        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="aa1d0fc807f708cfd98c49b61ef03159" />
        <meta-data
            android:name="com.kaolafm.open.sdk.AppKey"
            android:value="901e93ac3c7fefb21aec26d5e1c199a0" />
        <meta-data
            android:name="com.kaolafm.open.sdk.AppId"
            android:value="ne3560" />

        <meta-data
            android:name="com.kaolafm.open.sdk.Channel"
            android:value="xtev" />

        <receiver android:name=".module.map.receiver.MapReceiver">
            <intent-filter>

                <!-- 接收高德地图广播信息 -->
                <action android:name="AUTONAVI_STANDARD_BROADCAST_SEND" />
            </intent-filter>
        </receiver>
        <service
            android:name=".module.music.service.MusicService"
            android:enabled="true"
            android:exported="true"
            android:process=":music"></service> <!-- <service android:name="com.kaolafm.sdk.core.statistics.StatisticsService" /> -->
        <service android:name="com.kaolafm.sdk.core.mediaplayer.PlayerService" />
        <service android:name="com.kaolafm.sdk.core.mediaplayer.VLCMediaPlayService" />
        <service android:name="com.amap.api.location.APSService" />
        <service
            android:name=".module.video.VideoPlayService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.sitechdev.vehicle.video.service" />
            </intent-filter>

        </service>
        <!--
        <meta-data
                    android:name="com.kaolafm.open.sdk.AppKey"
                    android:value="431d5b0b5183e4757aa0465b10fca525" />
                <meta-data
                    android:name="com.kaolafm.open.sdk.AppId"
                    android:value="tm9908" />
                <meta-data
                    android:name="com.kaolafm.open.sdk.Channel"
                    android:value="xtev" />
        -->
        <!-- 高德定位组件 -->
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>
    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission
        android:name="android.permission.WRITE_SETTINGS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.MODIFY_PHONE_STATE" />

</manifest>