<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/id_main_popup_view_content"
    android:layout_width="match_parent"
    android:layout_height="500pt"
    android:layout_margin="13pt"
    android:background="@drawable/bg_control_panel_v"
    android:paddingLeft="40pt"
    android:paddingTop="40pt"
    android:paddingRight="40pt">

    <LinearLayout
        android:id="@+id/id_top_content"
        android:layout_width="match_parent"
        android:layout_height="30pt"
        android:layout_alignParentBottom="true"
        android:gravity="center"
        android:paddingBottom="15pt">

        <ImageView
            android:id="@+id/id_top_image"
            android:layout_width="240pt"
            android:layout_height="15pt"
            android:gravity="center_vertical"
            android:src="@drawable/main_control_top_control" />
    </LinearLayout>


    <RelativeLayout
        android:id="@+id/id_first_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="20pt"
        android:background="@drawable/control_panel_switch_bg"
        android:padding="45pt">

        <ImageView
            android:id="@+id/id_wifi_Btn"
            android:layout_width="134pt"
            android:layout_height="134pt"
            android:layout_alignParentLeft="true"
            android:clickable="true"
            android:src="@drawable/selector_control_wifi" />

        <ImageView
            android:id="@+id/id_mobile_net_Btn"
            android:layout_width="134pt"
            android:layout_height="134pt"
            android:layout_marginLeft="32pt"
            android:layout_toRightOf="@id/id_wifi_Btn"
            android:clickable="true"
            android:src="@drawable/selector_control_mobile_net" />

        <ImageView
            android:id="@+id/id_teddy_Btn"
            android:layout_width="134pt"
            android:layout_height="134pt"
            android:layout_below="@id/id_wifi_Btn"
            android:layout_marginTop="28pt"
            android:clickable="true"
            android:src="@drawable/selector_control_teddy" />

        <ImageView
            android:id="@+id/id_bluetooth_Btn"
            android:layout_width="134pt"
            android:layout_height="134pt"
            android:layout_below="@id/id_mobile_net_Btn"
            android:layout_marginLeft="32pt"
            android:layout_marginTop="28pt"
            android:layout_toRightOf="@id/id_teddy_Btn"
            android:clickable="true"
            android:src="@drawable/selector_control_bluetooth" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/id_second_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="48pt"
        android:layout_marginRight="40pt"
        android:layout_toRightOf="@id/id_first_content">

        <FrameLayout
            android:id="@+id/id_screen_light_change_content"
            android:layout_width="match_parent"
            android:layout_height="134pt"
            android:background="@drawable/bg_popup_btn_view">

            <SeekBar
                android:id="@+id/id_control_light_seek_bar"
                android:layout_width="match_parent"
                android:layout_height="134pt"
                android:background="@drawable/bg_popup_btn_view"
                android:max="10"
                android:paddingStart="0dp"
                android:paddingEnd="0dp"
                android:progress="5"
                android:progressDrawable="@drawable/layer_list_sound_bar"
                android:thumb="@null" />

            <ImageView
                android:layout_width="65pt"
                android:layout_height="65pt"
                android:layout_gravity="center_vertical|left"
                android:layout_marginLeft="40pt"
                android:src="@drawable/ico_control_light" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|right"
                android:layout_marginRight="40pt"
                android:text="亮度"
                android:textColor="#80566A93"
                android:textSize="28pt" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/id_volume_change_content"
            android:layout_width="match_parent"
            android:layout_height="134pt"
            android:layout_below="@id/id_screen_light_change_content"
            android:layout_marginTop="54pt">

            <SeekBar
                android:id="@+id/id_control_volume_seek_bar"
                android:layout_width="match_parent"
                android:layout_height="134pt"
                android:background="@drawable/bg_popup_btn_view"
                android:max="30"
                android:paddingStart="0dp"
                android:paddingEnd="0dp"
                android:progress="10"
                android:progressDrawable="@drawable/layer_list_sound_bar"
                android:thumb="@null" />

            <ImageView
                android:layout_width="65pt"
                android:layout_height="65pt"
                android:layout_gravity="center_vertical|left"
                android:layout_marginLeft="40pt"
                android:src="@drawable/ico_control_volume" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|right"
                android:layout_marginRight="40pt"
                android:text="音量"
                android:textColor="#80566A93"
                android:textSize="28pt" />

        </FrameLayout>

    </RelativeLayout>

