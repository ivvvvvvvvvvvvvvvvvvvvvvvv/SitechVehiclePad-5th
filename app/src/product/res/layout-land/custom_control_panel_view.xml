<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/id_main_popup_view_content"
    android:layout_width="@dimen/control_panel_view_h_width"
    android:layout_height="match_parent"
    android:layout_marginLeft="30pt"
    android:layout_marginTop="30pt"
    android:layout_marginBottom="30pt"
    android:background="@drawable/bg_control_panel">

    <LinearLayout
        android:id="@+id/id_top_content"
        android:layout_width="30pt"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:gravity="center"
        android:visibility="visible">

        <ImageView
            android:id="@+id/id_top_image"
            android:layout_width="15pt"
            android:layout_height="240pt"
            android:gravity="center_vertical"
            android:src="@drawable/main_control_top_control" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/id_first_content"
        android:layout_width="347pt"
        android:layout_height="347pt"
        android:layout_marginLeft="30pt"
        android:layout_marginTop="30pt"
        android:layout_marginRight="30pt"
        android:layout_marginBottom="15pt"
        android:layout_toLeftOf="@id/id_top_content"
        android:background="@drawable/control_panel_switch_bg"
        android:padding="30pt">

        <ImageView
            android:id="@+id/id_wifi_Btn"
            android:layout_width="127pt"
            android:layout_height="127pt"
            android:layout_alignParentLeft="true"
            android:clickable="true"
            android:scaleType="centerInside"
            android:src="@drawable/selector_control_wifi" />

        <ImageView
            android:id="@+id/id_mobile_net_Btn"
            android:layout_width="127pt"
            android:layout_height="127pt"
            android:layout_marginLeft="33pt"
            android:layout_toRightOf="@id/id_wifi_Btn"
            android:clickable="true"
            android:scaleType="centerInside"
            android:src="@drawable/selector_control_mobile_net" />

        <ImageView
            android:id="@+id/id_teddy_Btn"
            android:layout_width="127pt"
            android:layout_height="127pt"
            android:layout_below="@id/id_wifi_Btn"
            android:layout_marginTop="30pt"
            android:clickable="true"
            android:scaleType="centerInside"
            android:src="@drawable/selector_control_teddy" />

        <ImageView
            android:id="@+id/id_bluetooth_Btn"
            android:layout_width="127pt"
            android:layout_height="127pt"
            android:layout_below="@id/id_mobile_net_Btn"
            android:layout_marginLeft="33pt"
            android:layout_marginTop="30pt"
            android:layout_toRightOf="@id/id_teddy_Btn"
            android:clickable="true"
            android:scaleType="centerInside"
            android:src="@drawable/selector_control_bluetooth" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/id_second_content"
        android:layout_width="347pt"
        android:layout_height="match_parent"
        android:layout_below="@id/id_first_content"
        android:layout_marginLeft="30pt"
        android:layout_marginTop="15pt"
        android:layout_marginRight="30pt"
        android:layout_marginBottom="30pt"
        android:layout_toLeftOf="@id/id_top_content"
        android:background="@drawable/control_panel_switch_bg"
        android:padding="30pt">

        <FrameLayout
            android:id="@+id/id_screen_light_change_content"
            android:layout_width="128pt"
            android:layout_height="match_parent">

            <com.sitechdev.vehicle.pad.view.VerticalSeekBarForSkin
                android:id="@+id/id_control_light_seek_bar"
                android:layout_width="128pt"
                android:layout_height="match_parent"
                android:background="@drawable/bg_popup_btn_view"
                android:progressDrawable="@drawable/layer_list_sound_bar_land" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|top"
                android:layout_marginTop="30pt"
                android:text="亮度"
                android:textColor="#80566A93"
                android:textSize="28pt" />

            <ImageView
                android:layout_width="57pt"
                android:layout_height="57pt"
                android:layout_gravity="center_horizontal|bottom"
                android:layout_marginBottom="27pt"
                android:src="@drawable/ico_control_light" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/id_volume_change_content"
            android:layout_width="128pt"
            android:layout_height="match_parent"
            android:layout_marginLeft="30pt"
            android:layout_toRightOf="@id/id_screen_light_change_content">

            <com.sitechdev.vehicle.pad.view.VerticalSeekBarForSkin
                android:id="@+id/id_control_volume_seek_bar"
                android:layout_width="128pt"
                android:layout_height="match_parent"
                android:background="@drawable/bg_popup_btn_view"
                android:progressDrawable="@drawable/layer_list_sound_bar_land" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|top"
                android:layout_marginTop="30pt"
                android:text="音量"
                android:textColor="#80566A93"
                android:textSize="28pt" />

            <ImageView
                android:layout_width="57pt"
                android:layout_height="57pt"
                android:layout_gravity="center_horizontal|bottom"
                android:layout_marginBottom="27pt"
                android:src="@drawable/ico_control_volume" />

        </FrameLayout>

    </RelativeLayout>


